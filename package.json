  "bugs": {
    "url": "https://github.com/GabrielFernella/clean-node-api/issues"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "homepage": "https://github.com/GabrielFernella/clean-node-api#readme",
  "devDependencies": {
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "mongoose": "^6.9.0",
    "standard": "^17.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "validator": "^13.7.0"
  }
}
